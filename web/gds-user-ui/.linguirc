{
  "locales": [
    "de",
    "en",
    "es",
    "fr",
    "ja",
    "pt",
    "zh",
    "en-dh"
  ],
  "format": "po",
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "./src/locales/{locale}/messages",
      "include": [
        "src"
      ],
      "exclude": [
        "**/node_modules/**"
      ]
    }
  ],
  "rootDir": "."
}
